{
	"name": "naga-la-decryption",
	"version": "0.0.0",
	"description": "Naga Lit Action decryption example",
	"repository": {
		"type": "git",
		"url": "git+https://github.com//naga-la-decryption.git"
	},
	"license": "MIT",
	"type": "module",
	"main": "lib/index.js",
	"files": [
		"lib/"
	],
	"scripts": {
		"build": "tsdown",
		"format": "prettier . --write",
		"lint": "eslint . --max-warnings 0",
